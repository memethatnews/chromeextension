{
  "name": "Meme That News",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon48.png"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://memethatnews.com https://cdn.embedly.com https://embed.redditmedia.com; object-src 'self'"
}
