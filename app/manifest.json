{
  "name": "Meme That News",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "See if a news article has already been memed on Reddit!",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "Check Memes for this page on Reddit",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://cdn.embedly.com https://embed.redditmedia.com; object-src 'self'"
}
